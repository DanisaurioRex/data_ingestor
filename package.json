{
  "name": "@data-ingestor/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx build api && nx build web",
    "build:api": "nx build api",
    "build:web": "nx build web",
    "build:all": "nx run-many --target=build --all",
    "start": "nx serve api",
    "start:api": "nx serve api",
    "start:web": "nx serve web",
    "start:dev": "concurrently \"nx serve api\" \"nx serve web\"",
    "start:prod": "node dist/api/main.js",
    "serve:api": "nx serve api --port=3000",
    "serve:web": "nx serve web --port=4200",
    "debug:api": "nx serve api --inspect",
    "debug:web": "nx serve web --inspect",
    "test": "nx test api && nx test web",
    "test:api": "nx test api",
    "test:web": "nx test web",
    "test:all": "nx run-many --target=test --all",
    "test:watch": "nx test api --watch",
    "test:watch:api": "nx test api --watch",
    "test:watch:web": "nx test web --watch",
    "test:coverage": "nx test api --coverage && nx test web --coverage",
    "test:coverage:api": "nx test api --coverage",
    "test:coverage:web": "nx test web --coverage",
    "e2e": "nx e2e api-e2e && nx e2e web-e2e",
    "e2e:api": "nx e2e api-e2e",
    "e2e:web": "nx e2e web-e2e",
    "e2e:headless": "nx e2e api-e2e --headless && nx e2e web-e2e --headless",
    "lint": "nx run-many --target=lint --all",
    "lint:api": "nx lint api",
    "lint:web": "nx lint web",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "nx reset",
    "graph": "nx graph",
    "affected:build": "nx affected --target=build",
    "affected:test": "nx affected --target=test",
    "affected:lint": "nx affected --target=lint",
    "affected:e2e": "nx affected --target=e2e",
    "dep-graph": "nx dep-graph",
    "prepare": "husky install || true",
    "precommit": "lint-staged",
    "postinstall": "nx build api"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@types/multer": "^2.0.0",
    "axios": "^1.6.0",
    "multer": "^2.0.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nx/devkit": "21.2.2",
    "@nx/eslint": "21.2.2",
    "@nx/eslint-plugin": "21.2.2",
    "@nx/jest": "21.2.2",
    "@nx/js": "21.2.2",
    "@nx/nest": "21.2.2",
    "@nx/node": "21.2.2",
    "@nx/playwright": "21.2.2",
    "@nx/react": "^21.2.2",
    "@nx/vite": "21.2.2",
    "@nx/web": "21.2.2",
    "@nx/webpack": "21.2.2",
    "@nx/workspace": "21.2.2",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "~0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.0.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^3.0.0",
    "concurrently": "^8.2.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "lint-staged": "^15.0.0",
    "nx": "21.2.2",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.0",
    "webpack-cli": "^5.1.4"
  },
  "workspaces": [
    "apps/*",
    "api",
    "api-e2e",
    "web"
  ]
}
